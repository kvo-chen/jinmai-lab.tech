# Created by Vercel CLI
# 请复制此文件为 .env.development 或 .env.production 并填入实际值

# Autonoma 配置
AUTONOMA_CLIENT_ID="your-client-id"
AUTONOMA_SECRET_ID="your-secret-id"

# CORS 配置
CORS_ALLOW_ORIGIN="*"

# 数据库配置
DATABASE_URL="postgresql://user:password@host:port/database?sslmode=require"
DB_TYPE="postgresql"

# JWT 配置
JWT_EXPIRES_IN="7d"
JWT_SECRET="your-secret-key-change-this-in-production"

# Neon 数据库配置
NEON_API_KEY="your-neon-api-key"
NEON_POSTGRES_DATABASE_URL="postgresql://user:password@host:port/database?sslmode=require"
NEON_POSTGRES_DATABASE_URL_UNPOOLED="postgresql://user:password@host:port/database?sslmode=require"
NEON_POSTGRES_NEON_PROJECT_ID="your-project-id"
NEON_POSTGRES_PGDATABASE="database"
NEON_POSTGRES_PGHOST="host"
NEON_POSTGRES_PGHOST_UNPOOLED="host"
NEON_POSTGRES_PGPASSWORD="password"
NEON_POSTGRES_PGUSER="user"
NEON_POSTGRES_POSTGRES_DATABASE="database"
NEON_POSTGRES_POSTGRES_HOST="host"
NEON_POSTGRES_POSTGRES_PASSWORD="password"
NEON_POSTGRES_POSTGRES_PRISMA_URL="postgresql://user:password@host:port/database?connect_timeout=15&sslmode=require"
NEON_POSTGRES_POSTGRES_URL="postgresql://user:password@host:port/database?sslmode=require"
NEON_POSTGRES_POSTGRES_URL_NON_POOLING="postgresql://user:password@host:port/database?sslmode=require"
NEON_POSTGRES_POSTGRES_URL_NO_SSL="postgresql://user:password@host:port/database"
NEON_POSTGRES_POSTGRES_USER="user"

# Vercel OIDC 配置
VERCEL_OIDC_TOKEN="your-vercel-oidc-token"

# Supabase 配置
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-supabase-anon-key
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Next.js 兼容性配置
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-supabase-anon-key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# 通义千问 API 配置
QWEN_API_KEY=your-qwen-api-key
