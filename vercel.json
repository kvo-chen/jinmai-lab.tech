{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/static",
        "buildCommand": "npm run build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/chunks/(.*)",
      "destination": "/chunks/$1"
    },
    {
      "source": "/entries/(.*)",
      "destination": "/entries/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}