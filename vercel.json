{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/static",
        "buildCommand": "npm run build"
      }
    }
  ],
  "functions": {
    "api/auth/login.ts": {
      "maxDuration": 10
    },
    "api/auth/me.ts": {
      "maxDuration": 10
    },
    "api/auth/register.ts": {
      "maxDuration": 10
    },
    "api/categories/[id].ts": {
      "maxDuration": 10
    },
    "api/categories/index.ts": {
      "maxDuration": 10
    },
    "api/comments/[id].ts": {
      "maxDuration": 10
    },
    "api/comments/index.ts": {
      "maxDuration": 10
    },
    "api/likes/index.ts": {
      "maxDuration": 10
    },
    "api/posts/[id].ts": {
      "maxDuration": 10
    },
    "api/posts/index.ts": {
      "maxDuration": 10
    },
    "api/tags/[id].ts": {
      "maxDuration": 10
    },
    "api/tags/index.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/chunks/(.*)",
      "destination": "/chunks/$1"
    },
    {
      "source": "/entries/(.*)",
      "destination": "/entries/$1"
    },
    {
      "source": "/manifest.webmanifest",
      "destination": "/manifest.webmanifest"
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/workbox-(.*)",
      "destination": "/workbox-$1"
    },
    {
      "source": "/registerSW.js",
      "destination": "/registerSW.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}