{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "pnpm build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/chunks/(.*)",
      "destination": "/chunks/$1"
    },
    {
      "source": "/entries/(.*)",
      "destination": "/entries/$1"
    },
    {
      "source": "/manifest.webmanifest",
      "destination": "/manifest.webmanifest"
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/workbox-(.*)",
      "destination": "/workbox-$1"
    },
    {
      "source": "/registerSW.js",
      "destination": "/registerSW.js"
    },
    {
      "source": "/api/proxy/trae-api/(.*)",
      "destination": "https://trae-api-sg.mchost.guru/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}